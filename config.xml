<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.cordovaapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Teste Captura</name>
    <description>
        Testando Captura corporal via app
    </description>
    <author email="dev@example.com" href="http://example.com">
        Developer
    </author>
    <content src="index.html" />
    <access origin="*" />
